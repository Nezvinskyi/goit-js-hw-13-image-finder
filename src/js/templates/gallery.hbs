{{#each this}}
<li>
	<div class="gallery__item">
		<a class="gallery__link" href="{{largeImageURL}}">
			<img class="gallery__image" src="{{webformatURL}}" alt="photo: {{tags}}">
		</a>
		<div class="stats">
			<p class="stats-item">
				<i class="material-icons">thumb_up</i>
				<span class="stats-text"> {{likes}}</span>


			</p>
			<p class="stats-item">
				<i class="material-icons"> visibility</i>
				<span class="stats-text">{{views}}</span>
			</p>
			<p class="stats-item">
				<i class="material-icons">comment</i>
				<span class="stats-text">{{comments}}</span>

			</p>
			<p class="stats-item">
				<i class="material-icons">cloud_download</i>
				<span class="stats-text">{{downloads}}</span>
			</p>
		</div>
	</div>
</li>
{{/each}}